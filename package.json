{
    "name": "sass-index-creator",
    "displayName": "sass-index-creator",
    "description": "When you create an index.scss it will automatically populate it with imports for all the sass files in the current directory, and all the index.scss files in the sub-directories.",
    "version": "0.0.1",
    "publisher": "q",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "scss",
        "sass",
        "import",
        "index",
        "template",
        "generator",
        "imports"
    ],
    "activationEvents": [
        "onLanguage:scss"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
